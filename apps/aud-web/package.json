    "@vercel/og": "^0.8.5",
    "clsx": "^2.1.0",
    "date-fns": "^3.0.0",
    "framer-motion": "^11.0.0",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.546.0",
    "nanoid": "^5.1.6",
    "next": "15.5.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "react-hotkeys-hook": "^5.2.1",
