{
  "name": "aud-web",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=9999",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .next",
    "test": "playwright test",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:report": "playwright show-report tests/output/report",
    "test:personas": "playwright test tests/personas/persona-tests.spec.ts",
    "test:personas:headed": "playwright test tests/personas/persona-tests.spec.ts --headed",
    "test:persona": "playwright test tests/personas/persona-tests.spec.ts --grep",
    "seed:persona": "npx ts-node tests/personas/seed-content.ts",
    "seed:personas": "npx ts-node tests/personas/seed-content.ts --all"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.0",
