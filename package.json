{
  "name": "totalaud.io",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=aud-web",
    "dev:clean": "pnpm clean && pnpm install && pnpm dev",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=aud-web",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json}\"",
    "typecheck": "turbo run typecheck",
    "typecheck:web": "turbo run typecheck --filter=aud-web",
    "clean": "turbo run clean && rm -rf node_modules",
    "check": "pnpm typecheck && pnpm lint && pnpm format:check",
    "check:fast": "pnpm typecheck:web && pnpm lint",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:restart": "supabase stop && supabase start",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:types": "supabase gen types typescript --local > packages/schemas/database/types.ts",
    "db:studio": "open http://localhost:54323",
    "silver:check": "tsx scripts/silver-deploy.ts",
    "silver:rollback": "tsx scripts/silver-rollback.ts",
    "port:kill": "lsof -ti:3000 | xargs kill -9 || true"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@octokit/rest": "^22.0.0",
    "@supabase/supabase-js": "^2.39.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "dotenv": "^17.2.3",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "prettier": "^3.6.2",
    "tsx": "^4.7.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.0"
}
